{
  "name": "tempmail",
  "version": "1.0.0",
  "description": "Temp-mail.org SDK - 创建临时邮箱和获取邮件",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && bun build src/index.ts --outdir dist --target node --format cjs --outfile dist/index.cjs",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "tempmail",
    "temp-mail",
    "temporary-email",
    "disposable-email"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "axios": "^1.13.5"
  }
}